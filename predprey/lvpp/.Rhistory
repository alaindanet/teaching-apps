f <- function(which,Ni,Nj,params) {
with (as.list(params),  {
i <- (lambda.i)/(1+alpha.ii*gi*Ni+alpha.ij*gj*Nj)
j <- (lambda.j)/(1+alpha.jj*gj*Nj+alpha.ji*gi*Ni)
if (which == "i") {return (i)}
else {return (j)}
})
}
Nt <- function (Ni, Nj, t, params) {
with (as.list(params), {
vectNi <- numeric(t)
vectNi[1] <- Ni
vectNj <- numeric(t)
vectNj[1] <- Nj
for (i in 2:t) {
vectNi[i] <- (1-gi)*si*vectNi[i-1] + gi*vectNi[i-1]*f(which = "i", Ni=vectNi[i-1], Nj=vectNj[i=1],params=params)
}
for (i in 2:t) {
vectNj[i] <- (1-gj)*sj*vectNj[i-1] + gj*vectNj[i-1]*f(which = "j", Ni=vectNi[i-1], Nj=vectNj[i=1],params=params)
}
return(as.data.frame(list("Ni"=vectNi,"Nj"=vectNj)))
})
}
