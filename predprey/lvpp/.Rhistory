# import data matrices, copied from Angert et al 2009 (PNAS)
library(vegan)
response <- read.csv("~/grad/research/sedgwick/Angert_2009_matrices/response.csv",row.names="X")
traits <- read.csv("~/grad/research/sedgwick/Angert_2009_matrices/traits.csv",row.names="X")
####################################################
# Set up four matrices by decomposing Angert's two #
####################################################
response.to.year <- matrix(nrow=9,ncol=9)
rownames(response.to.year) <- rownames(response)
colnames(response.to.year) <- colnames(response)
for (i in 1:9) {
for (j in 1:i){
response.to.year[i,j] <- response[j,i]
}
}
response.to.precip <- matrix(nrow=9,ncol=9)
rownames(response.to.precip) <- rownames(response)
colnames(response.to.precip) <- colnames(response)
for (i in 1:9) {
for (j in 1:i){
response.to.precip[i,j] <- response[i,j]
}
}
all.traits <- matrix(nrow=9,ncol=9)
rownames(all.traits) <- rownames(response)
colnames(all.traits) <- colnames(response)
for (i in 1:9) {
for (j in 1:i){
all.traits[i,j] <- traits[i,j]
}
}
key.traits <- matrix(nrow=9,ncol=9)
rownames(key.traits) <- rownames(response)
colnames(key.traits) <- colnames(response)
for (i in 1:9) {
for (j in 1:i){
key.traits[i,j] <- traits[j,i]
}
}
mantel(all.traits,response.to.year,permutations = 9999)
mantel(all.traits,response.to.year,permutations = 4000)
mantel(key.traits,response.to.year,permutations = 4000)
as.dist(all.traits)
cor(as.dist(all.traits),as.dist(response.to.year))
